<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uns.paysys.modules.sys.dao.ActionTypeDao">
	
	<select id="findAllList" resultType="ActionType">
		SELECT 
			* 
		FROM action_type 
		WHERE status = 1
		ORDER BY id
	</select>
	
	

</mapper>